{
    "name": "icecave/temptation",
    "description": "Painless temporary files and directories that clean up after themselves.",
    "keywords": [
        "tmp",
        "temp",
        "folder",
        "dir",
        "directory",
        "file"
    ],
    "homepage": "https://github.com/icecave/temptation",
    "license": "MIT",
    "authors": [
        {
            "name": "James Harris",
            "email": "mailjamesharris@gmail.com",
            "homepage": "https://github.com/jmalloc"
        }
    ],
    "require": {
        "php": "^7.1",
        "symfony/filesystem": "^2",
        "icecave/isolator": "^2"
    },
    "require-dev": {
        "phake/phake": "^3",
        "phpunit/phpunit": "^7"
    },
    "autoload": { "psr-4": { "Icecave\\Temptation\\": "src" } },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "7.1"
        }
    }
}
