{
    "name": "icecave/temptation",
    "description": "Painless temporary files and directories that clean up after themselves.",
    "keywords": [
        "tmp",
        "temp",
        "folder",
        "dir",
        "directory",
        "file"
    ],
    "homepage": "https://github.com/IcecaveStudios/temptation",
    "license": "MIT",
    "authors": [
        {
            "name": "James Harris",
            "email": "james.harris@icecave.com.au",
            "homepage": "https://github.com/jmalloc"
        }
    ],
    "require": {
        "php": ">=5.3",
        "symfony/filesystem": "~2",
        "icecave/isolator": "~2"
    },
    "require-dev": {
        "icecave/archer": "~1"
    },
    "autoload": {
        "psr-4": {
            "Icecave\\Temptation\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "1.0.x-dev"
        }
    }
}
